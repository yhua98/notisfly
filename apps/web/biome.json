{
	"extends": ["../../biome.json"],
	"linter": {
		"rules": {
			"recommended": true,
			"correctness": {
				"noUnusedVariables": {
					"level": "error",
					"fix": "none"
				},
				"noUnusedImports": {
					"level": "error",
					"fix": "none"
				}
			},
			"style": {
				"useConst": {
					"level": "warn",
					"fix": "unsafe"
				},
				"useTemplate": {
					"level": "warn",
					"fix": "safe"
				}
			}
		}
	}
}
